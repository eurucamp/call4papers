<%= simple_form_for [@call, @proposed_speaker] do |f| %>
  <%= f.error_notification %>

  <div class="inputs">
    <%= f.input :speaker_name %>
    <%= f.input :speaker_email %>
    <%= f.input :reason %>
  </div>

  <div class="actions">
    <%= f.button :submit, class: 'btn btn-info' %>
  </div>
<% end %>
